		:AnswerMessageNumber2	"SK02A_Q1A2","『みんなで行けばいいんじゃない？』"
		:Message0Flag 	"coco_rt","=",0
		:Message1Flag 	"coco_rt","=",0
		:Message2Flag	"coco_pts","+",1
		:Frame		1
	}
