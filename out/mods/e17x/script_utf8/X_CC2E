	[Task16700]
	{
		:Tasktype	"Window"
		:Visible	0
	}
//5pb.-デフォルト停止時間60f-
	[Task16710]
	{
		:Tasktype	"BgmStop"
		:Div		60
	}
	[Task16720]
	{
		:Tasktype		"FadeOut"
		:Type			0
		:Color			0,0,0,255	//Black
		:Time			60
		:SpriteDelete	1
	}
	[Task16730]
	{
		:Tasktype	"SeStop"
		:Div		0
	}
	[Task16740]
	{
		:Tasktype	"BgmStop"
		:Div		0
	}
//5pb.--初期化コマンド--
	[Task16750]
	{
		:Tasktype	"CallScene"
		:Name		"sho_com","InitTag"
	}
//⇒X_CC3_1
//5pb.-ファイル移動-
	[Task16760]
	{
		:Tasktype	"NextScene"
		:NextScene	"X_CC3A"
	}
//アクアシステムデフォルトスクリプト--ここから--
	[Task16770]
	{
		:Tasktype		"End"
	}
}
