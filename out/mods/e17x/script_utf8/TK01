//『つぐみはどこに行ったのか？』
//*以降ココルートの選択肢は発生せず、ココルートには進入できません
//つぐみポイント+２
//『空はどこにいるのだろう？』
//*以降ココルートの選択肢は発生せず、ココルートには進入できません
//空ポイント+１
//『なんであの時、悲しくなったのだろう？』
//ココポイント+１
	[Task22680]
	{
		:Tasktype	"MessageSwitch_Tag"
		:AnswerMax 3
		:AnswerMessageNumber0	"TK01_Q6A0","『つぐみはどこに行ったのか？』"
		:AnswerMessageNumber1	"TK01_Q6A1","『空はどこにいるのだろう？』"
		:AnswerMessageNumber2	"TK01_Q6A2","『なんであの時、悲しくなったのだろう？』"
		:Message0Flag 	"coco_rt","=",0
		:Message1Flag 	"coco_rt","=",0
		:Message2Flag 	"coco_pts","+",1
		:Frame		0
	}
