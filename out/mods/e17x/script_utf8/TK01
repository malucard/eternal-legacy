	}
//『さすがに疲れたか』
//*以降ココルートの選択肢は発生せず、ココルートには進入できません
//『ほんとに眠いだけか？』
//ココポイント+１
	[Task20460]
	{
		:Tasktype	"MessageSwitch_Tag"
		:AnswerMax 2
		:AnswerMessageNumber0	"TK01_Q5A0","『さすがに疲れたか』"
		:AnswerMessageNumber1	"TK01_Q5A1","『ほんとに眠いだけか？』"
		:Message0Flag 	"coco_rt","=",0
		:Message1Flag 	"coco_pts","+",1
		:Frame		0
	}
