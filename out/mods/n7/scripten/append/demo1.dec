@label ap_START
@choice Izumi or Kurumi|Yuka or Haruka|Saki or Okuhiko|Quit
@setvar vap_selvar = selected
@if selvar == 0
@    goto ap_izumi_kurumi
@fi
@if selvar == 1
@    goto ap_yuuka_haruka
@fi
@if selvar == 2
@    goto ap_saki_okuhiko
@fi
@if selvar == 3
@    goto ap_exit
@fi
@goto ap_start
@label ap_izumi_kurumi
@choice Izumi|Kurumi
@setvar vap_selvar = selected
@if selvar == 0
@    goto ap_izumi
@fi
@if selvar == 1
@    goto ap_kurumi
@fi
@label ap_yuuka_haruka
@choice Yuka|Haruka
@setvar vap_selvar = selected
@if selvar == 0
@    goto ap_yuka
@fi
@if selvar == 1
@    goto ap_haruka
@fi
@label ap_saki_okuhiko
@choice Saki|Okuhiko
@setvar vap_selvar = selected
@if selvar == 0
@    goto ap_saki
@fi
@if selvar == 1
@    goto ap_okuhiko
@fi
